style "filer"
{
        bg[NORMAL] = { 0.95, 0.95, 0.95}
        fg[NORMAL] = { 0.0, 0.0, 0.0 }
	/*
	 * Used for selected items when the filer does NOT own
	 * the primary selection.
	 */
